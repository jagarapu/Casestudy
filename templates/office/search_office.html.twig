{{ form_start(form, {attr: {novalidate: 'novalidate'}}) }}
<div class="contact-search-box">
    <div class="row">
        <div class="col-md-3">
            {{ form_row(form.city) }}
        </div>
    </div>
    <div class="row float-right">
                        <span class="m-r-5">
                            {{ form_row(form.search) }}
                        </span>
        <a href="{{ path('office_list') }}" class="btn btn-primary text-nowrap" value="Export">Clear
            Search</a>
        </span>
    </div>
</div>
{{ form_end(form) }}